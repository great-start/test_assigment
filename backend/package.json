{
   "name": "test_assignment",
   "version": "1.0.0",
   "description": "",
   "main": "dist/app.ts",
   "scripts": {
      "watch:server": "nodemon src/app.ts --watch ./src",
      "compile": "rimraf dist && tsc-watch --onSuccess 'npm run watch:server'",
      "lint": "eslint --cache --ext .js,.ts src",
      "lint:fix": "npm run lint -- --fix",
      "start": "ts-node src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs -d src/data-source.ts",
      "migration:generate": "npm run typeorm migration:generate `./src/migration/${npm_config_name}`",
      "migration:show": "npm run typeorm migration:show",
      "migration:run": "npm run typeorm migration:run",
      "migration:revert": "npm run typeorm migration:revert",
      "migration:create": "typeorm-ts-node-commonjs migration:create src/migration/$npm_config_name"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "dotenv": "^16.0.0",
      "express": "^4.18.1",
      "express-fileupload": "^1.3.1",
      "joi": "^17.6.0",
      "jsonwebtoken": "^8.5.1",
      "pg": "^8.7.3",
      "reflect-metadata": "^0.1.13",
      "typeorm": "^0.3.6",
      "uuid": "^8.3.2",
      "tinify": "^1.6.1"
   },
   "devDependencies": {
      "@types/express": "^4.17.13",
      "@types/express-fileupload": "^1.2.2",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/node": "^16.11.35",
      "@types/nodemon": "^1.19.1",
      "@types/uuid": "^8.3.4",
      "@typescript-eslint/eslint-plugin": "^5.22.0",
      "@typescript-eslint/parser": "^5.22.0",
      "eslint": "^8.15.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-plugin-import": "^2.26.0",
      "nodemon": "^2.0.16",
      "rimraf": "^3.0.2",
      "ts-node": "^10.7.0",
      "tsc-watch": "^5.0.3",
      "typescript": "4.5.2"
   }
}
